{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/**/*",
          "node_modules/.prisma/client/libquery_engine-rhel-openssl-1.0.x.so.node"
        ]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "api/index.js" }]
}
